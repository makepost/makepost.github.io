{
  "private": "true",
  "scripts": {
    "build": "wintersmith build",
    "configure": "node bin/configure",
    "deploy": "msg=\"$(echo `git log -1 --pretty=%B`)\" && wintersmith build && git push && cd build && git add . && git commit -m \"$msg\" && git push",
    "format": "stylefmt -r 'contents/**/*.css'",
    "start": "wintersmith preview"
  },
  "dependencies": {
    "moment": "2.3.x",
    "typogr": "0.5.x",
    "underscore": "1.4.x"
  },
  "devDependencies": {
    "stylefmt": "^6.0.0",
    "wintersmith": "^2.4.1"
  }
}
